  "metadata": {
    "description": "Station configuration for production line",
    "fields": {
      "number": "Unique station identifier (int)",
      "tank": "Physical tank number - tank can contain multiple stations, tank number = lowest station number in the tank (int)",
      "group": "Process group number - stations with same group are interchangeable (int)",
      "name": "Station name/type (string)",
      "x_position": "Physical position in mm along the line (int)",
      "y_position": "Physical position in mm perpendicular to line (int)",
      "z_position": "Physical position in mm vertical height (int)",
      "dropping_time": "Time in seconds to drop/lift parts at this station (int)",
      "station_type": "0 = dry station (buffer/dryer), 1 = wet station (treatment tank) (int)",
      "device_delay": "Additional device-specific delay in seconds (float)"
    },
    "relationships": {
      "tank_to_station": "1:n (one tank can contain multiple stations)",
      "station_to_tank": "n:1 (each station belongs to exactly one tank)",
      "tank_numbering": "Tank number = lowest station number within that tank",
      "group_numbering": "Group number = lowest station number in the interchangeable group"
    },
    "notes": [
      "Tank hierarchy: A tank can contain one or more stations at different positions within the tank",
      "Tank number is determined by the lowest station number it contains",
      "Group represents process interchangeability - stations with same group can be used for same process step",
      "Example: Tank 104 contains Stations 104, 105, 106 (three dryer positions in one physical tank)",
      "Example: Tank 119 contains Stations 119, 120 (two buffer positions in one physical tank)",
      "Position coordinates in millimeters: x = along line, y = perpendicular, z = vertical"
    ]
  }
